<?xml version="1.0" encoding="utf-8"?>
<bundle name="WooVIP">
  <domain name="woovip-lite">
    <project name="WooVIP" slug="woovip">
      <source>
        <directory>.</directory>
				<exclude>
					<directory>assets</directory>
					<directory>modules</directory>
				</exclude>
      </source>
      <target>
        <directory>languages</directory>
      </target>
      <template>
        <file>languages/woovip-lite.pot</file>
      </template>
    </project>
  </domain>
  <domain name="lws-adminpanel">
    <project name="LWS AdminPanel" slug="lws-adminpanel">
      <source>
        <directory>./assets/lws-adminpanel</directory>
      </source>
      <target>
        <directory>./assets/lws-adminpanel/languages</directory>
      </target>
      <template>
        <file>./assets/lws-adminpanel/languages/lws-adminpanel.pot</file>
      </template>
    </project>
  </domain>
  <domain name="woovip-pro">
    <project name="WooVIP Pro" slug="woovip-pro">
      <source>
        <directory>./modules/woovip-pro</directory>
      </source>
      <target>
        <directory>./modules/woovip-pro/languages</directory>
      </target>
      <template>
        <file>./modules/woovip-pro/languages/woovip-pro.pot</file>
      </template>
    </project>
  </domain>
  <exclude>
    <directory>./assets/tgm</directory>
  </exclude>
</bundle>
